



import random
from timeit import default_timer as timer
import time
start = timer()
from random import choice
import random
ww = ["😁 Have a great day"]*10 + ["🍕 slice to meet you!"]*10 + ["🚀 taking my procrastination to new heights"]*10 + ["⭐  Make today a great day!"]*10 + ["😴 get some rest!"]*10 + ["🌲 Adventure Outdoors!"]*10 + ["📚 Learn Something new!"]*10 + ["🏆 Try a new hobby!"]*10 + ["☃ Build A snowman!"]*10 + ["🍽 Try A new food!"]*10 + ["🕥 take time for yourself"]*10 + ["🕶 you found a secret!"]*1
def xyx():
    print(random.choice(ww))


name = input("whats your name?")
print("hi" + " " + name)

print("""
Generate an Emoji and text?
[1] Yes
[2] No
""")
choose = input("")
if choose == "1":
    xyx()
time.sleep(1)
print("""
Want to generate one random text?
[1] Yes
[2] No
""")
choose2 = input("")
if choose2 == "1":
    things = ["hi", "hello", ":)"]
print(random.choice(things))
choice_one = choice(things)
input("want to see the random text again? YES or NO")
if "YES":
    print(choice_one)
else:
    print(":(")

    
    


    


end = timer()
print(end - start)

